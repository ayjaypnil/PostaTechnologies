<!-- Include the Partial for the header including all of the necessary links -->
<%- include('partials/header') -%>

    <style>
        #customerSupportPageHeroImage {
            background-image: url("/images/customerSupportPageHero.png");
            background-color: white;
            height: 70vw;
            width: 100%;
            background-position: center center;
            background-repeat: no-repeat;
            background-size: contain;
            padding: 0px !important;
            display: block;
            margin: 0px !important;
            overflow: hidden;
        }

        #supportContentDiv {
            margin: 5vw 0;
        }

        /* For  Support Icons  */

        .customerSupportIcon {
            height: 3vw;
            width: auto;
        }

        /* For Information Panels, before they are collapsed */

        .panel-default>.panel-heading {
            background-color: #b7eafe;
            color: white;
            font-family: 'Poppins', sans-serif;
        }

        /* For the actual information once the panel is expanded */

        .panel-default>.panel-heading+.panel-collapse>.panel-body {
            font-family: 'Arimo', sans-serif;
            font-size: 4vw;
            color: rgba(67, 67, 67, 0.557);
        }

        .textMDA {
            font-family: 'Poppins', sans-serif;
            color: white;
            border: solid 1px;
            background-color: #5f8ce768;
            padding: 1vw;
        }

        @media only screen and (max-width: 400px) {
            .customerSupportIcon {
                height: 7vw;
                width: auto;
                padding-bottom: 1vw;
            }
        }

        @media only screen and (min-width: 401px) and (max-width: 767px) {
            .customerSupportIcon {
                height: 6vw;
                width: auto;
                padding-bottom: 1vw;
            }

        }
    </style>

    <!-- Head Close -->
    </head>
    <!-- Body Start -->

    <body>
        <!-- Include the Partial for the preloader  -->
        <%- include('partials/preloader') -%>
            <!-- Include the Partial for the topbar (regular)-->
            <%- include('partials/topBar') -%>


                <div class="" id="customerSupportPageHeroImage">

                </div>



                <div class="container-fluid" id="supportContentDiv">
                    <div class="row ">
                        <center>
                            <div class="col-xs-4">
                                <p class="well" style="background:#b7eafe">
                                    <a href="#profile" aria-controls="profile" role="tab" data-toggle="tab" style="color: white;">
                                        <span>
                                            <img class="customerSupportIcon" src="/images/information.png">
                                        </span>
                                        <br> General

                                    </a>
